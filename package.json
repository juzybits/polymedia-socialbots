{
    "private": true,
    "version": "0.0.0",
    "name": "@polymedia/socialbots",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://github.com/juzybits/polymedia-socialbots",
    "description": "Bots for Twitter, Telegram, Discord",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-socialbots"
    },
    "keywords": [
        "polymedia",
        "social",
        "bots"
    ],
    "scripts": {
        "start-socialbots":      "rm -rf dist/ && tsc && pm2 start pm2.config.cjs --env dev",
        "start-socialbots-prod": "rm -rf dist/ && tsc && pm2 start pm2.config.cjs --env prod",
        "start-socialbots-node": "rm -rf dist/ && tsc && node dist/socialbots.js",
        "build": "rm -rf dist/ && tsc"
    },
    "dependencies": {
        "@mysten/sui.js": "latest",
        "@polymedia/suits": "latest"
    },
    "devDependencies": {
        "@types/node": "latest",
        "typescript": "latest"
    },
    "type": "module",
    "sideEffects": false
}
