{
    "private": true,
    "version": "0.0.0",
    "name": "@polymedia/socialbots",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://github.com/juzybits/polymedia-socialbots",
    "description": "Fetch Sui onchain events and post to social media",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-socialbots"
    },
    "keywords": [
        "polymedia",
        "social",
        "bots"
    ],
    "scripts": {
        "clean": "rm -rf dist/ node_modules/",
        "start-dev": "rm -rf dist/ && tsc && pm2 start pm2.config.cjs --env dev",
        "start-prod": "rm -rf dist/ && tsc && pm2 start pm2.config.cjs --env prod",
        "start-node": "rm -rf dist/ && tsc && node dist/main.js",
        "build": "rm -rf dist/ && tsc"
    },
    "dependencies": {
        "@mysten/sui.js": "latest",
        "@polymedia/suits": "latest",
        "dotenv": "^16.4.5"
    },
    "devDependencies": {
        "@types/node": "latest",
        "typescript": "latest"
    },
    "type": "module",
    "sideEffects": false
}
